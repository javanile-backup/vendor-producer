{
    "name": "javanile/producer",
    "description": "Produce your vendor code everywhere",
    "version": "0.3.4",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Francesco Bianco",
            "email": "bianco@javanile.org"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/javanile/producer"
        }
    ],
    "bin": [
        "bin/producer"
    ],
    "require": {
        "php": "^5.3.6 || >=7.0 <7.2",
        "danielstjules/stringy": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "3.*",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Javanile\\Producer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javanile\\Producer\\Tests\\": "tests"
        }
    }
}
