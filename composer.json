{
  "name": "javanile/producer",
  "description": "Produce your vendor code everywhere",
  "version": "0.0.18",
  "license": "MIT",
  "type": "application",
  "authors": [{ "name": "Francesco Bianco", "email": "bianco@javanile.org" }],
  "repositories": [{ "type": "git", "url": "https://github.com/Javanile/Producer" }],
  "autoload-dev": { "psr-4": { "Javanile\\Producer\\Tests\\": "tests" } },
  "autoload": {
    "classmap": [ "src/Producer.php" ],
    "psr-4": { "Javanile\\Producer\\Commands\\": "cmd" }
  },
  "require": {
    "php": "^5.3.6 || >=7.0 <7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "bin": ["bin/producer"]
}